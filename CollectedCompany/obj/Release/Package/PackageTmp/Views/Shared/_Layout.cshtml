<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-site-verification" content="lp-liu152GbVq_1RdwkVBk5ZLpifbnMqSYWeiUOD14M" />
    <title>@ViewBag.Title - Music City Games</title>
    @Styles.Render("~/Content/css")
    <link href="http://code.jquery.com/ui/1.11.1/themes/smoothness/jquery-ui.css" rel="stylesheet" type="text/css">
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Scripts/eskju.jquery.scrollflow.css" rel="stylesheet"/>
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    <script src="http://code.jquery.com/jquery-latest.min.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    <script src="http://code.jquery.com/ui/1.11.1/jquery-ui.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <script src="~/Scripts/jquery.mb.YTPlayer.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBXyQU-BjugbAT0RiFpeBnKAZs2rlPxN4s"></script>
    <script src="~/Scripts/eskju.jquery.scrollflow.js"></script>
    @RenderSection("Theme", required: false)
    
</head>
<body>
    <div id="page-wrapper">
        @{ Html.RenderPartial("_Header"); }
        @RenderBody()
        @{Html.RenderPartial("_Footer");}
    </div>

   
    @RenderSection("Scripts", required: false)
</body>
</html>
