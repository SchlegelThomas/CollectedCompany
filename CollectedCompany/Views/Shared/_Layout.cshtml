<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-site-verification" content="lp-liu152GbVq_1RdwkVBk5ZLpifbnMqSYWeiUOD14M" />

    <title>@ViewBag.Title - Music City Games</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- ==========================
        Fonts
    =========================== -->
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,200,200italic,300,300italic,400italic,600,600italic,700,700italic,900,900italic&amp;subset=latin,latin-ext' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600,700,900,800' rel='stylesheet' type='text/css'>

    <!-- ==========================
        CSS
    =========================== -->
    <link href="~/Content/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="http://code.jquery.com/ui/1.11.1/themes/smoothness/jquery-ui.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/dragtable.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/owl.carousel.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/animate.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/color-switcher.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/custom.css" rel="stylesheet" type="text/css">
    <link href="~/Content/assets/css/color/dark-blue.css" id="main-color" rel="stylesheet" type="text/css">
    <link href="~/Scripts/eskju.jquery.scrollflow.css" rel="stylesheet" />
</head>
<body>
    <div id="page-wrapper">
        @{ Html.RenderPartial("_Header"); }
        @RenderBody()
        @{Html.RenderPartial("_Footer");}
    </div>

    <script src="http://code.jquery.com/jquery-latest.min.js"></script>
    <script src="http://code.jquery.com/ui/1.11.1/jquery-ui.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <script src="~/Content/assets/js/bootstrap.min.js"></script>
    <script src="~/Content/assets/js/bootstrap-hover-dropdown.min.js"></script>
    <script src="~/Content/assets/js/SmoothScroll.js"></script>
    <script src="~/Content/assets/js/jquery.dragtable.js"></script>
    <script src="~/Content/assets/js/jquery.card.js"></script>
    <script src="~/Content/assets/js/owl.carousel.min.js"></script>
    <script src="~/Content/assets/js/twitterFetcher_min.js"></script>
    <script src="~/Content/assets/js/jquery.mb.YTPlayer.min.js"></script>
    <script src="~/Content/assets/js/color-switcher.js"></script>
    <script src="~/Scripts/eskju.jquery.scrollflow.js"></script>
    <script src="~/Content/assets/js/custom.js"></script>
    @RenderSection("Scripts", required:false);
</body>
</html>
